<?xml version="1.0" encoding="UTF-8"?>
<form>
	<fields name="attribs">
		<fieldset name="custom" label="Event Options">
			<field
				name="csc_location"
				type="text"
				label="Location"
				filter="STRING"
				description="Location of Event."
			/>
			<field
				name=""
				type="text"
				label="Date"
				readonly="true"
				default="- Created Date (Publishing) -"
				description="Event date based on Finish Publishing Date."
			/>
			<field
				name="csc_date_finish"
				type="calendar"
				label="Finish Date"
				format="%Y-%m-%d %H:%M"
				showtime="true"
				description="Time event will be finished"
			/>
      <field
				name=""
				type="text"
				label="Event Image"
				readonly="true"
				default="- Intro Image (Images and Links) -"
        description="Cover Image of Event."/>
		</fieldset>
	</fields>
</form>
