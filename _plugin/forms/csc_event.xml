<?xml version="1.0" encoding="UTF-8"?>
<form>
	<fields name="attribs">
		<fieldset name="custom" label="Event Options">
			<field
				name="csc_location"
				type="text"
				label="Location"
				filter="STRING"
				description="Location of Event."
			/>
			<field
				name=""
				type="text"
				label="Date"
				readonly="true"
				default="- Finish Publishing (Publishing) -"
				description="Event date based on Finish Publishing Date."
			/>
      <field
        name="csc_image"
        type="media"
        label="Event Image"
        description="Cover Image of Event."/>
		</fieldset>
	</fields>
</form>
